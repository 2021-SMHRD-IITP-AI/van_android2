<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".subscription_information">

    <EditText
        android:id="@+id/edt_join_name"
        android:layout_width="1324px"
        android:layout_height="140px"
        android:ems="10"
        android:gravity="right"
        android:inputType="textPersonName"
        android:textSize="45px"
        app:layout_constraintEnd_toStartOf="@+id/si_right_58px"
        app:layout_constraintHorizontal_bias="0.227"
        app:layout_constraintStart_toStartOf="@+id/si_left_58px"
        app:layout_constraintTop_toTopOf="@+id/guideline26" />

    <EditText
        android:id="@+id/edt_join_pw"
        android:layout_width="1324px"
        android:layout_height="140px"
        android:layout_marginTop="10dp"
        android:ems="10"
        android:gravity="right"
        android:inputType="textPassword"
        android:paddingBottom="20dp"
        android:textSize="45px"
        app:layout_constraintEnd_toStartOf="@+id/si_right_58px"
        app:layout_constraintHorizontal_bias="0.227"
        app:layout_constraintStart_toStartOf="@+id/si_left_58px"
        app:layout_constraintTop_toBottomOf="@+id/edt_join_name" />

    <EditText
        android:id="@+id/edt_join_pw_ch"
        android:layout_width="1324px"
        android:layout_height="140px"
        android:layout_marginTop="10dp"
        android:ems="10"
        android:gravity="right"
        android:inputType="textPassword"
        android:paddingBottom="20dp"
        android:textSize="45px"
        app:layout_constraintEnd_toStartOf="@+id/si_right_58px"
        app:layout_constraintHorizontal_bias="0.227"
        app:layout_constraintStart_toStartOf="@+id/si_left_58px"
        app:layout_constraintTop_toBottomOf="@+id/edt_join_pw" />

    <EditText
        android:id="@+id/edt_join_phone"
        android:layout_width="1324px"
        android:layout_height="140px"
        android:layout_marginTop="10dp"
        android:ems="10"
        android:gravity="right"
        android:inputType="phone"
        android:textSize="45px"
        app:layout_constraintEnd_toStartOf="@+id/si_right_58px"
        app:layout_constraintHorizontal_bias="0.227"
        app:layout_constraintStart_toStartOf="@+id/si_left_58px"
        app:layout_constraintTop_toBottomOf="@+id/edt_join_pw_ch" />

    <EditText
        android:id="@+id/edt_join_date"
        android:layout_width="1324px"
        android:layout_height="140px"
        android:layout_marginTop="10dp"
        android:ems="10"
        android:gravity="right"
        android:inputType="date"
        android:textSize="45px"
        app:layout_constraintEnd_toStartOf="@+id/si_right_58px"
        app:layout_constraintStart_toStartOf="@+id/si_left_58px"
        app:layout_constraintTop_toBottomOf="@+id/edt_join_phone" />

    <Button
        android:id="@+id/btn_join"
        android:layout_width="1440px"
        android:layout_height="190px"
        android:background="#1F2A83"
        android:text="가입하기"
        android:textColor="#FFFFFF"
        android:textSize="60px"
        app:layout_constraintBottom_toTopOf="@+id/si_btn_under"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent" />

    <RadioGroup
        android:id="@+id/rd_group"
        android:layout_width="150dp"
        android:layout_height="52dp"
        android:layout_marginTop="292dp"
        android:gravity="right"
        android:orientation="horizontal"
        app:layout_constraintEnd_toStartOf="@+id/si_right_58px"
        app:layout_constraintTop_toTopOf="parent">

        <RadioButton
            android:id="@+id/rd_sex_m"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="20dp"
            android:layout_marginRight="20dp"
            android:text="남성"
            android:textSize="45px" />

        <RadioButton
            android:id="@+id/rd_sex_w"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="여성"
            android:textSize="45px" />
    </RadioGroup>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/si_left_58px"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="58px" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/si_right_58px"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_end="58px" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="192px" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginLeft="5dp"
        android:layout_marginBottom="5dp"
        android:text="이름"
        android:textSize="45px"
        app:layout_constraintBottom_toBottomOf="@+id/edt_join_name"
        app:layout_constraintStart_toStartOf="@+id/edt_join_name"
        app:layout_constraintTop_toTopOf="@+id/guideline26" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginLeft="5dp"
        android:layout_marginBottom="20dp"
        android:text="비밀번호"
        android:textSize="45px"
        app:layout_constraintBottom_toBottomOf="@+id/edt_join_pw"
        app:layout_constraintStart_toStartOf="@+id/si_left_58px"
        app:layout_constraintTop_toTopOf="@+id/edt_join_pw" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginLeft="5dp"
        android:layout_marginBottom="20dp"
        android:text="비밀번호 확인"
        android:textSize="45px"
        app:layout_constraintBottom_toBottomOf="@+id/edt_join_pw_ch"
        app:layout_constraintStart_toStartOf="@+id/si_left_58px"
        app:layout_constraintTop_toTopOf="@+id/edt_join_pw_ch" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginLeft="5dp"
        android:layout_marginBottom="20dp"
        android:text="핸드폰 번호"
        android:textSize="45px"
        app:layout_constraintBottom_toBottomOf="@+id/edt_join_phone"
        app:layout_constraintStart_toStartOf="@+id/si_left_58px"
        app:layout_constraintTop_toTopOf="@+id/edt_join_phone" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginLeft="5dp"
        android:layout_marginBottom="20dp"
        android:text="생년월일"
        android:textSize="45px"
        app:layout_constraintBottom_toBottomOf="@+id/edt_join_date"
        app:layout_constraintStart_toStartOf="@+id/si_left_58px"
        app:layout_constraintTop_toTopOf="@+id/edt_join_date" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="12dp"
        android:text="성별"
        android:textSize="45px"
        app:layout_constraintStart_toStartOf="@+id/si_left_58px"
        app:layout_constraintTop_toBottomOf="@+id/edt_join_date" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/si_btn_under"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_end="97px" />

</androidx.constraintlayout.widget.ConstraintLayout>